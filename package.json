{
  "name": "@youdotcom-oss/n8n-nodes-youdotcom",
  "version": "0.2.5",
  "description": "n8n community node for You.com APIs - Search and Contents extraction",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youdotcom-oss/n8n-nodes-youdotcom.git"
  },
  "bugs": {
    "url": "https://github.com/youdotcom-oss/n8n-nodes-youdotcom/issues"
  },
  "homepage": "https://github.com/youdotcom-oss/n8n-nodes-youdotcom#readme",
  "author": {
    "name": "You.com",
    "email": "tyler@you.com",
    "url": "https://you.com"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "you.com",
    "search",
    "web search",
    "ai search",
    "contents",
    "scrape",
    "crawl"
  ],
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build.ts && bun run build:assets",
    "build:assets": "cp nodes/YouDotCom/youdotcom.svg dist/nodes/YouDotCom/ && cp nodes/YouDotCom/YouDotCom.node.json dist/nodes/YouDotCom/ && cp nodes/YouDotCom/youdotcom.svg dist/credentials/",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "check": "biome check && tsc --noEmit",
    "check:write": "biome check --write",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/YouDotComApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/YouDotCom/YouDotCom.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "^2.6.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@n8n/node-cli": "^0.19.0",
    "@types/bun": "^1.3.9",
    "eslint": "9.32.0",
    "n8n-workflow": "^2.6.0",
    "typescript": "^5.9.3",
    "zod": "^3.25.67"
  }
}
